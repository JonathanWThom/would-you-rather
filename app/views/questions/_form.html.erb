<%= form_for @question, remote:true do |f| %>
  <%= f.label "Your Question" %>
  <%= f.text_field :content %>
  <%= f.label "Option 1" %>
  <%= f.text_field :option_1 %>
  <%= f.label "Photo 1" %>
  <%= f.text_field :photo_1 %>
  <%= f.label "Option 2" %>
  <%= f.text_field :option_2 %>
  <%= f.label "Photo 2" %>
  <%= f.text_field :photo_2 %>
  <%= f.submit "Create Question" %>
<% end %>
